{
  "name": "jh-invoice",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "zipDownload": "cd src/lambda/invoice-download && npm install && chmod -R 777 node_modules && zip -r invoice-download.zip *",
    "postzipDownload": "mv src/lambda/invoice-download/invoice-download.zip lambda-build",
    "zipHarvest": "cd src/lambda/harvest && npm install && chmod -R 777 node_modules && zip -r harvest.zip *",
    "postzipHarvest": "mv src/lambda/harvest/harvest.zip lambda-build",
    "prebuild": "rm -rf lambda-build && mkdir lambda-build && npm run zipDownload && npm run zipHarvest",
    "postbuild": "cp redirects.txt dist/_redirects",
    "start:lambda:harvest": "netlify-lambda serve src/lambda/harvest --config ./webpack.functions.js --port 9001",
    "start:lambda:download": "netlify-lambda serve src/lambda/invoice-download --config ./webpack.functions.js --port 9002",
    "start:lambda": "netlify-lambda serve src/lambda",
    "test": "echo \"Error: no test specified, but use test:unit instead\" && exit 1",
    "newinstall": "yarn install --modules-folder ./directory_location"
  },
  "dependencies": {
    "chrome-aws-lambda": "^2.0.1",
    "core-js": "^3.4.4",
    "dotenv": "^8.2.0",
    "harvest-v2": "^1.2.0",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "puppeteer-core": "^2.0.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-body-class": "^3.0.2",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-unit-mocha": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "hygen": "^5.0.3",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.12.0",
    "puppeteer": "^2.0.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
